<!-- <title>Exploración</title> -->

<statement>
  <p>
    Resuelve estas ecuaciones:
  </p>
  <ol>
    <li>
      <p>
        <m>4a = 32</m>
      </p>
    </li>
    <li>
      <p>
        <m>4 = 32b</m>
      </p>
    </li>
    <li>
      <p>
        <m>10c = 26</m>
      </p>
    </li>
    <li>
      <p>
        <m>26 = 100d</m>
      </p>
    </li>
  </ol>
</statement>

<solution>
  <ol>
    <li>
      <p>
        <m>a = 8</m>
      </p>
    </li>
    <li>
      <p>
        <m>b = \frac{1}{8}</m>
      </p>
    </li>
    <li>
      <p>
        <m>c = \num{2.6}</m> (o algo equivalente)
      </p>
    </li>
    <li>
      <p>
        <m>d = \num{0.6}</m> (o algo equivalente)
      </p>
    </li>
  </ol>
</solution>

</exercise>